@inherits Microsoft.VisualStudio.Web.CodeGeneration.Templating.RazorTemplateBase
using MediatR;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Threading.Tasks;

namespace @(Model.Namespace).Application.Command
{
    public class @(Model.CommandClassName)Command : IRequest<double>
    {
        public string Name { get; set; }
        public @(Model.CommandClassName)Command(string name)
        {
            Name = name;
        }
    }
}